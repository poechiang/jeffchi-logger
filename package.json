{
  "name": "@jeffchi/logger",
  "version": "4.0.1-alpha.1",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "browser": "lib/index.cjs",
  "types": "lib/index.d.ts",
  "type": "module",
  "description": "A log print output javascript tool library that can be used at the front and back ends",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poechiang/jeffchi-logger.git"
  },
  "bugs": {
    "url": "https://github.com/poechiang/jeffchi-logger/issues"
  },
  "homepage": "https://github.com/poechiang/jeffchi-logger#readme",
  "keywords": [
    "typescript",
    "javascript",
    "logger",
    "assert",
    "success",
    "log",
    "warn",
    "info",
    "error",
    "debug"
  ],
  "scripts": {
    "clear": "rm -rf lib/ docs/ build/",
    "doc": "typedoc --options typedoc.json",
    "dev": "rollup -c --watch",
    "build": "npm run clear && rollup -c",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "test:main": "jest ./src/__tests__/main.test.ts --config jestconfig.json",
    "prepush": "git add .",
    "push": "git commit -m",
    "postpush": "git push",
    "prepublishOnly": "npm run test main && npm run lint",
    "publish": "git push --follow-tags origin develop && npm publish",
    "release": "node release"
  },
  "author": "jeffery·chiang",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "✨ Features | 新功能"
      },
      {
        "type": "fix",
        "section": "🐛 Bug Fixes | Bug 修复"
      },
      {
        "type": "init",
        "section": "🎉 Init | 初始化"
      },
      {
        "type": "docs",
        "section": "✏️ Documentation | 文档"
      },
      {
        "type": "style",
        "section": "💄 Styles | 风格"
      },
      {
        "type": "refactor",
        "section": "♻️ Code Refactoring | 代码重构"
      },
      {
        "type": "perf",
        "section": "⚡ Performance Improvements | 性能优化"
      },
      {
        "type": "test",
        "section": "✅ Tests | 测试"
      },
      {
        "type": "revert",
        "section": "⏪ Revert | 回退",
        "hidden": true
      },
      {
        "type": "build",
        "section": "📦‍ Build System | 打包构建"
      },
      {
        "type": "chore",
        "section": "🚀 Chore | 构建/工程依赖/工具"
      },
      {
        "type": "ci",
        "section": "👷 Continuous Integration | CI 配置"
      }
    ],
    "skip": {
      "bump": true,
      "changelog": true,
      "commit": true,
      "tag": true
    },
    "scripts": {
      "prerelease": "npm run lint && npm run format && git add -A ."
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.11.18",
    "@types/standard-version": "^7.0.1",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.0",
    "jest": "^29.3.1",
    "prettier": "^3.1.1",
    "rollup": "^3.10.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "shelljs": "^0.8.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
