{
  "name": "@jeffchi/logger",
  "version": "2.2.0",
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "type": "module",
  "description": "A log print output javascript tool library that can be used at the front and back ends",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poechiang/jeffchi-logger.git"
  },
  "bugs": {
    "url": "https://github.com/poechiang/jeffchi-logger/issues"
  },
  "homepage": "https://github.com/poechiang/jeffchi-logger#readme",
  "keywords": [
    "typescript",
    "javascript",
    "logger",
    "log",
    "warn",
    "info",
    "error",
    "debug"
  ],
  "scripts": {
    "clear": "rm -rf lib/ docs/ build/",
    "doc": "typedoc --options typedoc.json",
    "dev": "rollup -c --watch",
    "build": "npm run clear && rollup -c",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "push": "git add . && git commit -m",
    "postpush": "git push",
    "prepublishOnly": "npm run test main && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add .",
    "postversion": "git push && git push --tags",
    "release": "standard-version --release-as",
    "postrelease": "git push --follow-tags && npm publish",
    "release:alpha": "standard-version --prerelease alpha --release-as",
    "postrelease:alpha": "git push --follow-tags && npm publish --tag alpha",
    "release:rc": "standard-version --prerelease rc --release-as",
    "postrelease:rc": "git push --follow-tags && npm publish --tag rc"
  },
  "author": "jefferyÂ·chiang",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "lib",
    "*.d.ts"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "babel-preset-env": "^1.7.0",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.2",
    "rollup": "^3.10.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  }
}
